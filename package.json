{
    "private": true,
    "type": "module",
    "engines": {
        "node": ">=18.20.5",
        "npm": "*",
        "yarn": "please-use-npm",
        "pnpm": "please-use-npm",
        "bun": "please-use-npm"
    },
    "scripts": {
        "build": "vue-tsc && vite build",
        "dev": "vite",
        "lint:check": "eslint resources/js --ext .js,.ts,.vue --ignore-path .gitignore",
        "lint:fix": "eslint resources/js --ext .js,.ts,.vue --ignore-path .gitignore --fix",
        "format:check": "prettier --check resources/js/**/*.{js,ts,vue} --cache",
        "format:fix": "prettier --write resources/js/**/*.{js,ts,vue} --cache",
        "code:check": "npm run lint:check && npm run format:check",
        "code:fix": "npm run lint:fix && npm run format:fix",
        "test": "echo 'No tests yet.'",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@inertiajs/vue3": "1.2.0",
        "@primevue/auto-import-resolver": "4.2.3",
        "@rushstack/eslint-patch": "1.10.4",
        "@tailwindcss/forms": "0.5.9",
        "@types/luxon": "3.4.2",
        "@vitejs/plugin-vue": "5.2.0",
        "@vue/eslint-config-prettier": "9.0.0",
        "@vue/eslint-config-typescript": "13.0.0",
        "autoprefixer": "10.4.20",
        "axios": "1.7.7",
        "concurrently": "9.1.0",
        "eslint": "8.57.1",
        "eslint-plugin-vue": "9.31.0",
        "husky": "8.0.3",
        "laravel-vite-plugin": "1.0.6",
        "postcss": "8.4.49",
        "prettier": "3.3.3",
        "prettier-plugin-organize-imports": "4.1.0",
        "prettier-plugin-tailwindcss": "0.6.9",
        "tailwindcss": "3.4.15",
        "typescript": "5.6.3",
        "unplugin-vue-components": "0.27.4",
        "vite": "5.4.11",
        "vue": "3.5.13",
        "vue-tsc": "2.1.10"
    },
    "dependencies": {
        "@phosphor-icons/web": "2.1.1",
        "@primevue/themes": "4.2.3",
        "@tanstack/vue-query": "5.61.3",
        "luxon": "3.5.0",
        "primevue": "4.2.3",
        "vee-validate": "4.14.7",
        "yup": "1.4.0"
    }
}
