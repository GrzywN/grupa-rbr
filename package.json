{
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vue-tsc && vite build",
        "dev": "vite",
        "lint:check": "eslint resources/js --ext .js,.ts,.vue --ignore-path .gitignore",
        "lint:fix": "eslint resources/js --ext .js,.ts,.vue --ignore-path .gitignore --fix",
        "format:check": "prettier --check resources/js/**/*.{js,ts,vue} --cache",
        "format:fix": "prettier --write resources/js/**/*.{js,ts,vue} --cache",
        "code:check": "npm run lint:check && npm run format:check",
        "code:fix": "npm run lint:fix && npm run format:fix",
        "test": "echo 'No tests yet.'"
    },
    "devDependencies": {
        "@inertiajs/vue3": "^1.0.0",
        "@rushstack/eslint-patch": "^1.8.0",
        "@tailwindcss/forms": "^0.5.3",
        "@vitejs/plugin-vue": "^5.0.0",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/eslint-config-typescript": "^13.0.0",
        "autoprefixer": "^10.4.12",
        "axios": "^1.7.4",
        "concurrently": "^9.0.1",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.23.0",
        "laravel-vite-plugin": "^1.0",
        "postcss": "^8.4.31",
        "prettier": "^3.3.0",
        "prettier-plugin-organize-imports": "^4.0.0",
        "prettier-plugin-tailwindcss": "^0.6.5",
        "tailwindcss": "^3.2.1",
        "typescript": "^5.5.3",
        "vite": "^5.0",
        "vue": "^3.4.0",
        "vue-tsc": "^2.0.24"
    }
}
